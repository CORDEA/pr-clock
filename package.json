    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^8.31.0",
    "eslint-plugin-github": "^4.6.0",
    "eslint-plugin-jest": "^27.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "jest-circus": "^24.9.0",
